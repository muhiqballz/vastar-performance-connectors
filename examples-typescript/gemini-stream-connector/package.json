{
  "name": "openai-stream-sse-connector-ts",
  "version": "1.0.0",
  "description": "OpenAI Stream SSE Connector using Vastar TypeScript SDK",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "start:simulator": "USE_SIMULATOR=true npm start",
    "start:openai": "USE_SIMULATOR=false npm start",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openai",
    "sse",
    "streaming",
    "vastar",
    "connector"
  ],
  "author": "Vastar Team",
  "license": "MIT",
  "dependencies": {
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.2"
  }
}

